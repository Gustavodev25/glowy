{
  "name": "project_build",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev --turbopack",
    "dev:default": "next dev",
    "dev:ngrok": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "start:ngrok": "next start --hostname 0.0.0.0",
    "lint": "eslint",
    "check-encoding": "node scripts/check-encoding.cjs",
    "pre-commit": "npm run check-encoding && npm run lint",
    "ngrok": "ngrok http 3000",
    "ngrok:dev": "concurrently \"npm run dev:ngrok\" \"ngrok http 3000\"",
    "ngrok:prod": "concurrently \"npm run start:ngrok\" \"ngrok http 3000\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "fix:enderecos": "tsx scripts/fix-endereco-empresas.ts",
    "debug:agendamentos": "tsx scripts/debug-agendamentos.ts",
    "check:last-agendamento": "tsx scripts/check-last-agendamento.ts",
    "test:query-agendamentos": "tsx scripts/test-query-agendamentos.ts"
  },
  "dependencies": {
    "@ark-ui/react": "^5.25.1",
    "@google/generative-ai": "^0.24.1",
    "@number-flow/react": "^0.5.10",
    "@prisma/client": "^6.17.1",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/three": "^0.180.0",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "gsap": "^3.13.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^3.15.0",
    "next": "15.5.4",
    "prisma": "^6.17.1",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-image-crop": "^11.0.10",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0",
    "twilio": "^4.22.0",
    "zod": "^4.1.11"
